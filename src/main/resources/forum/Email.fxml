            <Font name="Arial Nova Cond Light" size="21.0" />
         </font>
      </Button>
      <TextField fx:id="txtMsgArea" layoutX="127.0" layoutY="137.0" prefHeight="39.0" prefWidth="346.0" promptText="Email" />
   </children>
</AnchorPane>

<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<HBox prefHeight="59.0" prefWidth="522.0" spacing="30.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Label fx:id="id_Categories" alignment="CENTER" prefHeight="49.0" prefWidth="60.0" text="01">
         <font>
            <Font name="Corbel Bold" size="12.0" />
         </font>
         <padding>
            <Insets left="10.0" />
         </padding>
         <HBox.margin>
            <Insets top="5.0" />
         </HBox.margin>
      </Label>
      <Label fx:id="intitule_Categorie" prefHeight="59.0" prefWidth="260.0" text="test" textFill="#575757">
         <font>
            <Font name="Corbel Bold Italic" size="14.0" />
         </font>
         <padding>
            <Insets left="50.0" />
         </padding>
      </Label>
      <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" spacing="30.0">
         <children>
            <Button fx:id="delete_btn" layoutX="62.0" layoutY="10.0" mnemonicParsing="false" onAction="#handleDeleteButtonClick" prefHeight="20.0" prefWidth="20.0" style="-fx-background-color: red;" styleClass="buton" stylesheets="@style.css">
               <graphic>
                  <ImageView fitHeight="20.0" fitWidth="20.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@image/bin.png" />
                     </image>
                  </ImageView>
               </graphic>
            </Button>
            <Button fx:id="edit_btn" layoutX="10.0" layoutY="10.0" mnemonicParsing="false" onAction="#handleUpdateButtonClick" prefHeight="20.0" prefWidth="20.0" style="-fx-background-color: #ddce48;" styleClass="buton" stylesheets="@style.css">
               <graphic>
                  <ImageView fitHeight="20.0" fitWidth="20.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@image/write.png" />
                     </image>
                  </ImageView>
               </graphic>
            </Button>
         </children>
      </HBox>
   </children>
</HBox>

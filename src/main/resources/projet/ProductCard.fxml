<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<HBox prefHeight="52.0" prefWidth="508.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <ImageView fx:id="row_image" fitHeight="53.0" fitWidth="76.0" nodeOrientation="INHERIT" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@image/416314507_356678277125643_4479438506546698261_n.png" />
         </image>
         <HBox.margin>
            <Insets left="1.0" />
         </HBox.margin>
      </ImageView>
      <Label fx:id="row_intitule" alignment="CENTER" prefHeight="53.0" prefWidth="107.0" text="INTITULE" textFill="#575757">
         <font>
            <Font name="Corbel Bold Italic" size="14.0" />
         </font>
         <HBox.margin>
            <Insets />
         </HBox.margin>
      </Label>
      <Label fx:id="row_description" alignment="CENTER" layoutX="67.0" layoutY="20.0" prefHeight="53.0" prefWidth="120.0" text="DESCRIPSTION" textFill="#575757">
         <font>
            <Font name="Corbel Bold Italic" size="14.0" />
         </font>
         <padding>
            <Insets left="3.0" />
         </padding>
         <HBox.margin>
            <Insets />
         </HBox.margin>
      </Label>
      <Label fx:id="row_categorie" alignment="CENTER" layoutX="300.0" layoutY="10.0" prefHeight="53.0" prefWidth="99.0" text="CATEGORIE" textFill="#575757">
         <font>
            <Font name="Corbel Bold Italic" size="14.0" />
         </font>
         <padding>
            <Insets left="3.0" />
         </padding>
      </Label>
      <Label fx:id="row_prix" alignment="CENTER" layoutX="174.0" layoutY="10.0" prefHeight="53.0" prefWidth="57.0" text="PRIX" textFill="#575757">
         <font>
            <Font name="Corbel Bold Italic" size="14.0" />
         </font>
         <padding>
            <Insets left="3.0" />
         </padding>
      </Label>
      <Label fx:id="row_stock" alignment="CENTER" layoutX="273.0" layoutY="10.0" prefHeight="53.0" prefWidth="76.0" text="STOCK" textFill="#575757">
         <font>
            <Font name="Corbel Bold Italic" size="14.0" />
         </font>
         <padding>
            <Insets left="3.0" />
         </padding>
      </Label>
      <HBox alignment="CENTER" prefHeight="53.0" prefWidth="45.0" spacing="5.0">
         <children>
            <Button fx:id="delete_btn" layoutX="62.0" layoutY="10.0" mnemonicParsing="false" onAction="#handleDeleteButtonClick" prefHeight="28.0" prefWidth="31.0" style="-fx-background-color: red;" styleClass="buton" stylesheets="@style.css">
               <graphic>
                  <ImageView fitHeight="20.0" fitWidth="20.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@image/bin.png" />
                     </image>
                  </ImageView>
               </graphic>
            </Button>
            <Button fx:id="update_btn" layoutX="10.0" layoutY="10.0" mnemonicParsing="false" onAction="#handleUpdateButtonClick" prefHeight="20.0" prefWidth="20.0" style="-fx-background-color: #ddce48;" styleClass="buton" stylesheets="@style.css">
               <graphic>
                  <ImageView fitHeight="20.0" fitWidth="20.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@image/write.png" />
                     </image>
                  </ImageView>
               </graphic>
            </Button>
         </children>
      </HBox>
      <Label fx:id="row_count" text="Label" visible="false" />
   </children>
   <padding>
      <Insets left="3.0" />
   </padding>
</HBox>
